<?xml version="1.0" encoding="utf-8" ?>
<ProxyConfig allowedReferers="*"
                logFile="proxy_log.log"
                logLevel="INFO"
                mustMatch="true">
  <serverUrls>
		<serverUrl url="http://server:8080/arcgis"
				   matchAll="true"
				   httpBasicAuth="true"
				   username="111"
				   password="1111">
		</serverUrl>
    <serverUrl url="http://services.arcgisonline.com"
        matchAll="true"/>
  </serverUrls>
</ProxyConfig>

